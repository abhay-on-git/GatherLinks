{
    "manifest_version":3,
    "name":"GatherLinks",
    "version":"1.0.0",
    "description": "Gathering important links right away in your Browser",
    "host_permissions":["<all_urls>"],
    "permissions":[
        "activeTab","scripting","tabs","storage"
    ],
    "action":{
    "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/G-16.png",
        "32":"/images/G-32.png",
        "48":"/images/G-48.png",
        "128":"/images/G-128.png"
    }
}